alt + shift + Return
	alacritty

alt + a
	brave

alt + shift + a
	brave --incognito

alt + shift + q
	killall dwm xmonad

alt + p
	dmenu_run

F6
	xbacklight -ctrl intel_backlight -10

F7
	xbacklight -ctrl intel_backlight +10

alt + F6
	xbacklight -ctrl chromeos::kbd_backlight -10

alt + F7
	xbacklight -ctrl chromeos::kbd_backlight +10

F8
	amixer sset Master mute > /dev/null 2>&1

F9
	amixer sset Master 10%- > /dev/null 2>&1

F10
	amixer sset Master 10%+ unmute > /dev/null 2>&1
